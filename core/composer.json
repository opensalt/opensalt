{
    "name": "opensalt/opensalt",
    "license": "MIT",
    "type": "project",
    "description": "A competency framework management system that works with the CASE format",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "DoctrineMigrations\\": "migrations/",
            "LocalApp\\": "local-src/"
        },
        "exclude-from-classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Codeception\\Module\\": "tests/_support/Module"
        },
        "classmap": [
            "tests/_behat/Context.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/opensalt/codeception-email-mailtrap.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1.0",
        "ext-PDO": "*",
        "ext-bz2": "*",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "antishov/doctrine-extensions-bundle": "^1.4.3",
        "colinodell/json5": "^2.1",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.3.2",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.0.2",
        "doctrine/migrations": "^3.0.2",
        "doctrine/orm": "^2.8",
        "firebase/php-jwt": "^6.0",
        "gbprod/uuid-normalizer": "^1.1",
        "google/recaptcha": "~1.1",
        "guzzlehttp/guzzle": "^6.5|^7.3",
        "kamermans/guzzle-oauth2-subscriber": "^1.0.11",
        "laminas/laminas-code": "^4.0",
        "laminas/laminas-escaper": "^2.2",
        "league/csv": "^9.6",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/oauth2-github": "^3.0",
        "league/uri": "^6.0",
        "milo/github-api": "^2.0",
        "nelmio/cors-bundle": "^2.0",
        "nelmio/security-bundle": "^3.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpoffice/phpspreadsheet": "~1.23.0",
        "phpoffice/phpword": "^0.18.0",
        "qandidate/toggle-bundle": "^1.4.0",
        "ramsey/uuid": "^4.0",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^6.0",
        "swaggest/json-schema": "^0.12.20",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^2.0",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mercure-bundle": "^0.3.0",
        "symfony/mime": "5.4.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/security-csrf": "5.4.*",
        "symfony/serializer": "^6.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/yaml": "5.4.*",
        "tecnickcom/tcpdf": "^6.2",
        "tetranz/select2entity-bundle": "^3.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/string-extra": "^3.1",
        "twig/twig": "^3.1"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^2.0",
        "codeception/module-doctrine2": "^2.0",
        "codeception/module-phpbrowser": "^2.0",
        "codeception/module-rest": "^2.0",
        "codeception/module-sequence": "^2.0",
        "codeception/module-symfony": "^2.0",
        "codeception/module-webdriver": "^2.0",
        "codeception/specify": "^2.0",
        "codeception/verify": "^2.0",
        "ericmartel/codeception-email-mailtrap": "dev-master#afb827dc34506706f5aa9aa2b041219cc8d6b933 as 1.0.5.1",
        "jetbrains/phpstorm-attributes": "^1.0",
        "league/factory-muffin": "^3.3",
        "league/factory-muffin-faker": "^2.0",
        "phpunit/phpunit": "^9.1",
        "roave/security-advisories": "dev-master",
        "softcreatr/jsonpath": "~0.4",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "5.4.*",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*",
        "vlucas/phpdotenv": "^5.0"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}
